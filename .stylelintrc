{
  "extends": "stylelint-config-standard",
  "rules": {
    "rule-empty-line-before": "always-multi-line",
    "at-rule-no-unknown": [
      true,
      "ignoreAtRules": [
        "lost",
        "mixin",
        "add-mixin",
        "define-mixin"
      ]
    ],
    "property-no-unknown": [
      true,
      "ignoreProperties": [
        "lost-column"
      ]
    ],
  }
}
