{
  "name": "fronter",
  "version": "1.0.0",
  "description": "Front-end starter kit.",
  "homepage": "https://fronter.surge.sh",
  "license": "MIT",
  "engines": {
    "node": "~8.11.3",
    "npm": "~5.6.0"
  },
  "keywords": [
    "gulp",
    "webpack",
    "postcss",
    "pug"
  ],
  "author": "Lance Guyatt <lance@lanceguyatt.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:lanceguyatt/fronter.git"
  },
  "scripts": {
    "start": "node app.js",
    "dev": "gulp",
    "build": "NODE_ENV=production gulp build",
    "deploy": "npm run build && surge -p build -d fronter.surge.sh",
    "lint": "eslint 'src/**/*.js', 'gulpfile.babel.js/**/*.js', 'webpack/**/*.js', 'app.js', 'config.js', 'postcss.config.js'",
    "format": "prettier --write 'src/**/*.js' 'gulpfile.babel.js/*.js'",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.scss": ["postcss --config .stylelintrc --replace", "stylelint", "git add"],
    "*.js": ["prettier --write", "eslint --fix", "git add"]
  },
  "dependencies": {
    "dotenv": "^6.0.0",
    "serve": "^9.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.3",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "browser-sync": "^2.24.4",
    "css-mqpacker": "^6.0.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "gulp": "^3.9.1",
    "gulp-changed": "^3.2.0",
    "gulp-clean-css": "^3.9.4",
    "gulp-cli": "^2.0.1",
    "gulp-eslint": "^4.0.2",
    "gulp-htmlhint": "^2.1.1",
    "gulp-modernizr": "^2.0.4",
    "gulp-nodemon": "^2.2.1",
    "gulp-plumber": "^1.2.0",
    "gulp-postcss": "^7.0.1",
    "gulp-pug": "^4.0.1",
    "gulp-pug-lint": "^0.1.6",
    "gulp-rename": "^1.3.0",
    "gulp-sitemap": "^5.1.0",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-stylelint": "^7.0.0",
    "gulp-svgmin": "^1.2.4",
    "gulp-svgstore": "^6.1.1",
    "gulp-util": "^3.0.8",
    "husky": "^0.14.3",
    "jquery": "^3.3.1",
    "jstransformer-markdown-it": "^2.1.0",
    "lint-staged": "^7.2.0",
    "lost": "^8.3.0",
    "moment": "^2.22.2",
    "postcss-calc": "^6.0.1",
    "postcss-color-function": "^4.0.1",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^7.0.0",
    "postcss-custom-selectors": "^4.0.1",
    "postcss-import": "^11.1.0",
    "postcss-mixins": "^6.2.0",
    "postcss-nested": "^3.0.0",
    "postcss-short": "^4.1.0",
    "postcss-simple-vars": "^4.1.0",
    "prettier": "1.13.5",
    "run-sequence": "^2.2.1",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1",
    "svg4everybody": "^2.1.9",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.12.0"
  }
}
