{
  "name": "fronter",
  "version": "1.0.0",
  "description": "Front-end starter kit.",
  "homepage": "https://fronter.surge.sh",
  "license": "MIT",
  "keywords": [
    "gulp",
    "webpack",
    "postcss",
    "pug"
  ],
  "author": "Lance Guyatt <lance@lanceguyatt.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:lanceguyatt/fronter.git"
  },
  "scripts": {
    "serve": "http-server public -o",
    "dev": "gulp",
    "build": "NODE_ENV=production gulp build",
    "deploy": "npm run build && surge -p public -d fronter.surge.sh",
    "webpack:prod": "webpack-cli --mode=production --config ./webpack/webpack.config.prod.js",
    "js:diff": "prettier --config .prettierrc --list-different \"**/*.js\"",
    "js:format": "prettier --config .prettierrc --write  \"**/*.js\"",
    "js:test": "eslint --config .eslintrc.js \"**/*.js\"",
    "html:test": "htmlhint \"public**/*.html\"",
    "css:test": "stylelint \"**/*.css\" --fix --config .stylelintrc > foo.txt",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "pug:test": "pug-lint --config .puglintrc -reporter inline \"src**/*.pug\""
  },
  "lint-staged": {
    "*.js": [
      "prettier --config prettier.config.js --write",
      "git add"
    ]
  },
  "dependencies": {
    "http-server": "^0.11.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "browser-sync": "^2.26.7",
    "css-mqpacker": "^7.0.0",
    "dotenv": "^8.0.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-prettier": "^3.1.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-changed": "^4.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-clean-css": "^4.2.0",
    "gulp-cli": "^2.2.0",
    "gulp-htmlhint": "^2.2.1",
    "gulp-nodemon": "^2.4.2",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-pug": "^4.0.1",
    "gulp-pug-lint": "^0.1.6",
    "gulp-rename": "^1.4.0",
    "gulp-sitemap": "^7.2.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-stylelint": "^9.0.0",
    "gulp-svgmin": "^2.2.0",
    "gulp-svgstore": "^7.0.1",
    "gulp-util": "^3.0.8",
    "husky": "^2.4.1",
    "jquery": "^3.4.1",
    "jstransformer-markdown-it": "^2.1.0",
    "lint-staged": "^8.2.1",
    "lost": "^8.3.1",
    "moment": "^2.29.2",
    "postcss-calc": "^7.0.1",
    "postcss-cli": "^6.1.2",
    "postcss-color-function": "^4.1.0",
    "postcss-custom-media": "^7.0.8",
    "postcss-custom-properties": "^8.0.10",
    "postcss-custom-selectors": "^5.1.2",
    "postcss-import": "^12.0.1",
    "postcss-mixins": "^6.2.1",
    "postcss-nested": "^4.1.2",
    "postcss-short": "^5.0.0",
    "postcss-simple-vars": "^5.0.2",
    "prettier": "1.18.2",
    "run-sequence": "^2.2.1",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "svg4everybody": "^2.1.9",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4"
  }
}
